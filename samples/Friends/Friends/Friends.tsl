cell struct Character
{
	String Name;
	byte Gender;
	bool Married;
	long Spouse;
	long Performer;
}

cell struct Performer
{
	String Name;
	int Age;
	List<long> Characters;
}

cell struct Friendship
{
	List<long> friends;
}
