<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net6.0;net48</TargetFrameworks>
    <IsServiceFabricServiceProject>True</IsServiceFabricServiceProject>
    <ServerGarbageCollection>True</ServerGarbageCollection>
    <RuntimeIdentifier>win7-x64</RuntimeIdentifier>
    <TargetLatestRuntimePatch>False</TargetLatestRuntimePatch>
    <Platforms>AnyCPU;x64</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GraphEngine.Azure.BlobStorage" Version="2.0.10517" />
    <PackageReference Include="GraphEngine.Client" Version="2.0.10517" />
    <PackageReference Include="GraphEngine.DynamicCluster" Version="2.0.10517" />
    <PackageReference Include="GraphEngine.LIKQ" Version="2.0.10517" />
    <PackageReference Include="GraphEngine.ServiceFabric.Infrastructure" Version="2.0.10517" />
    <PackageReference Include="GraphEngine.ServiceFabric.Remoting" Version="2.0.10517" />
    <PackageReference Include="Microsoft.ServiceFabric.Services" Version="4.1.456" />
  </ItemGroup>

</Project>
